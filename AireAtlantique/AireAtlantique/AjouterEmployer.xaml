<Page x:Class="AireAtlantique.AjouterEmployer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AireAtlantique"
      mc:Ignorable="d" 
      d:DesignHeight="300"
      Title="AjouterEmployer" Width="384.221">

    <Grid Background="#FFAFBDC4" Margin="0,0,-46,0">
        <Button x:Name="button" Content="Home" HorizontalAlignment="Left" Margin="19,54,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Label x:Name="label" Content="Nom:" HorizontalAlignment="Left" Margin="186,74,0,0" VerticalAlignment="Top" Height="27" Width="99"/>
        <Label x:Name="label_Copy" Content="Prenom:" HorizontalAlignment="Left" Margin="186,141,0,0" VerticalAlignment="Top" Height="27" Width="99"/>
        <TextBox x:Name="tbNom" HorizontalAlignment="Left" Height="23" Margin="186,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" SpellCheck.IsEnabled="True"/>
        <TextBox x:Name="tbPrenom" HorizontalAlignment="Left" Height="23" Margin="186,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="bAjouter" Content="Ajouter" HorizontalAlignment="Left" Margin="219,251,0,0" VerticalAlignment="Top" Width="75" Click="bAjouter_Click"/>

    </Grid>
</Page>
