<Page x:Class="AireAtlantique.Parametre"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AireAtlantique"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="1050"
      Title="Parametre">

    <Grid Background="#EFEFEF" Height="700" Width="1050">
        <Button x:Name="bMenu" Content="Menu" HorizontalAlignment="Left" Margin="23,18,0,0" VerticalAlignment="Top" Width="75" Click="bMenu_Click"/>
        <TextBox x:Name="tbAjouterMetier" HorizontalAlignment="Left" Height="23" Margin="23,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="bAjouterMetier" Content="Ajouter Metier" HorizontalAlignment="Left" Margin="166,84,0,0" VerticalAlignment="Top" Width="131" Click="bAjouterMetier_Click"/>

    </Grid>
</Page>
